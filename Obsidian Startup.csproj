  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Images\obsidian-icon.ico" />
    <Resource Include="Images\obsidian-logo-text-white-purple.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\obsidian-icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Source\" />
  </ItemGroup>

  <ItemGroup>
    <Page Include="App.xaml" />
  </ItemGroup>

</Project>
